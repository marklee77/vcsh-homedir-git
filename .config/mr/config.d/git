[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/git.git]
checkout = vcsh clone https://gitlab.stillwell.me/mark/vcsh-homedir-git.git git
setpush  = vcsh git remote set-url --add --push origin git@stillwell.me:mark/vcsh-homedir-git.git
           vcsh git remote set-url --add --push origin git@github.com:marklee77/vcsh-homedir-git.git
           vcsh git remote set-url --add --push origin git@bitbucket.org:marklee77/vcsh-homedir-git.git
